{
  "favicon": "/public/favicon.ico",
  "oauth2": {
    "authorizationURL": "https://developer.api.autodesk.com/authentication/v1/authorize",
    "clientID": "<your Forge app ID>",
    "clientSecret": "<your Forge app secret>",
    "callbackURL": "/callback",
    "tokenURL": "https://developer.api.autodesk.com/authentication/v1/gettoken"
  },
  "http_port": 5000,
  "https_port": 5001,
  "scope": "[\"data:read\"]",
  "session": {
    "maxAge": 3600000,
    "overwrite": true,
    "httpOnly": true,
    "signed": true,
    "rolling": false,
    "renew": false
  },
  "GoogleAPIKey":"<your GoogleAPIKey>",
  "githubRepoUrl": "https://github.com/mazerab/consumption-reporting-vue-koa-app",
  "tlsversion": "TLSv1_2_method",
  "vuehost": "http://localhost:8080",
  "API_host": "https://developer.api.autodesk.com/tokenflex/v1",
  "logoutaccount_url": "https://accounts.autodesk.com/Authentication/LogOut"
}
